		public void Main()
		{
			// TODO: Add your code here
            string sourceDir = Dts.Variables["User::ParamInputFolder"].Value.ToString();
            DirectoryInfo dir = new DirectoryInfo(sourceDir);

            FileInfo[] headerFiles = dir.GetFiles("EclipseOrderHeader*.txt");

            DataTable dtFiles = new DataTable("FileList");
            dtFiles.Columns.Add("HeaderFile");
            dtFiles.Columns.Add("DetailFile");


            Array.Sort(headerFiles, (x, y) => StringComparer.OrdinalIgnoreCase.Compare(x.Name, y.Name));
            
            foreach (FileInfo file in headerFiles)
            {
                string fileName = file.Name;
                string headerFullPath = System.IO.Path.Combine(sourceDir, file.Name);
                
                    
                if (fileName.Contains("-"))
                {
                    string timeStamp = fileName.Substring(0, fileName.Length - 4);
                    timeStamp = timeStamp.Substring(timeStamp.Length - 15);

